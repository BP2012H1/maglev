<!DOCTYPE html>
<html lang="en">
  <head>
    <title>MagLev WebTools</title>
    <meta charset="utf-8">
    <link rel="Shortcut Icon" type="image/ico" href="images/favicon.ico">

    <link rel="stylesheet" href="stylesheets/reset.css"     type="text/css" media="screen">
    <link rel="stylesheet" href="stylesheets/webtools.css"  type="text/css" media="screen">

    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.11/jquery-ui.min.js"></script>
    <script type="text/javascript" src="javascript/webtools.js"></script>
    <script type="text/javascript" src="CodeMirror/js/codemirror.js"></script>
  </head>
  <body>
    <div id="rubyCodeBrowser" class="ui-widget-content">
      <div id="toolBar"></div>
      <div id="mainContent">
      <div id="rubyNavigation">
        <div id="modules" class="rubyList">
          <span class="caption">Modules &amp; Classes</span>
          <ul id="rubyModules"></ul>
        </div>
        
        <div id="constants" class="rubyList">
          <span class="caption">Constants</span>
          <ul id="rubyConstants"></ul>
        </div>

        <div id="moduleMethods" class="rubyList">
          <span class="caption">Module / Class Methods</span>
          <ul id="rubyModuleMethods"></ul>
        </div>

        <div id="instanceMethods" class="rubyList">
          <span class="caption">Instance Methods</span>
          <ul id="rubyInstanceMethods"></ul>
        </div>

      </div>
      <div id="rubyEditArea">
        <textArea id="rubyEditor" spellcheck="false"></textArea>
      </div>
      </div>
    </div>
    <div id="statusBar"></div>
  </body>
</html>
